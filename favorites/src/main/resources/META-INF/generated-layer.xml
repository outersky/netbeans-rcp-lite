<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN"
                            "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="org-netbeans-modules-favorites-Add.instance">
                <!--org.netbeans.modules.favorites.Actions.add()-->
                <attr
                    bundlevalue="org.netbeans.modules.favorites.Bundle#ACT_Add" name="displayName"/>
                <attr
                    methodvalue="org.netbeans.modules.favorites.Actions.add" name="instanceCreate"/>
            </file>
            <file name="org-netbeans-modules-favorites-Remove.instance">
                <!--org.netbeans.modules.favorites.Actions.remove()-->
                <attr
                    bundlevalue="org.netbeans.modules.favorites.Bundle#ACT_Remove" name="displayName"/>
                <attr
                    methodvalue="org.openide.awt.Actions.alwaysEnabled" name="instanceCreate"/>
                <attr
                    methodvalue="org.netbeans.modules.favorites.Actions.remove" name="delegate"/>
                <attr boolvalue="false" name="noIconInMenu"/>
            </file>
            <folder name="SelectDocumentNode">
                <file name="org-netbeans-modules-favorites-Select.instance">
                    <!--org.netbeans.modules.favorites.Actions.select()-->
                    <attr
                        bundlevalue="org.netbeans.modules.favorites.Bundle#ACT_Select_Main_Menu" name="displayName"/>
                    <attr
                        methodvalue="org.netbeans.modules.favorites.Actions.select" name="instanceCreate"/>
                </file>
            </folder>
            <file name="org-netbeans-modules-favorites-View.instance">
                <!--org.netbeans.modules.favorites.Tab.findDefault()-->
                <attr
                    methodvalue="org.openide.windows.TopComponent.openAction" name="instanceCreate"/>
                <attr
                    methodvalue="org.netbeans.modules.favorites.Tab.findDefault" name="component"/>
                <attr
                    bundlevalue="org.netbeans.modules.favorites.Bundle#ACT_View" name="displayName"/>
                <attr name="iconBase" stringvalue="org/netbeans/modules/favorites/resources/actionView.png"/>
            </file>
        </folder>
    </folder>
    <folder name="UI">
        <folder name="ToolActions">
            <folder name="Files">
                <file name="org-netbeans-modules-favorites-Add.shadow">
                    <!--org.netbeans.modules.favorites.Actions.add()-->
                    <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules-favorites-Add.instance"/>
                    <attr intvalue="300" name="position"/>
                </file>
            </folder>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="GoTo">
            <file name="org-netbeans-modules-favorites-SelectInFavorites.shadow">
                <!--org.netbeans.modules.favorites.Actions.select()-->
                <attr name="originalFile" stringvalue="Actions/Window/SelectDocumentNode/org-netbeans-modules-favorites-Select.instance"/>
                <attr intvalue="2800" name="position"/>
            </file>
        </folder>
        <folder name="Window">
            <file name="org-netbeans-modules-favorites-View.shadow">
                <!--org.netbeans.modules.favorites.Tab.findDefault()-->
                <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules-favorites-View.instance"/>
                <attr intvalue="400" name="position"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="favorites.settings">
                <!--org.netbeans.modules.favorites.Tab.getDefault()--><![CDATA[<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE settings PUBLIC "-//NetBeans//DTD Session settings 1.0//EN" "http://www.netbeans.org/dtds/sessionsettings-1_0.dtd">
<settings version="1.0">
  <instance class="org.netbeans.modules.favorites.Tab" method="getDefault"/>
</settings>
]]></file>
        </folder>
        <folder name="Modes">
            <folder name="explorer">
                <file name="favorites.wstcref">
                    <!--org.netbeans.modules.favorites.Tab.getDefault()-->
                    <attr intvalue="300" name="position"/><![CDATA[<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tc-ref PUBLIC "-//NetBeans//DTD Top Component in Mode Properties 2.0//EN" "http://www.netbeans.org/dtds/tc-ref2_0.dtd">
<tc-ref version="2.0">
  <tc-id id="favorites"/>
  <state opened="false"/>
</tc-ref>
]]></file>
            </folder>
        </folder>
    </folder>
</filesystem>
