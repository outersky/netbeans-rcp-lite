            <version>1.0.0</version>
        </dependency>

        <dependency>
            <groupId>cn.hillwind.netbeans</groupId>
            <artifactId>windows</artifactId>
            <version>1.0.0</version>
        </dependency>

        <dependency>
            <groupId>cn.hillwind.netbeans</groupId>
